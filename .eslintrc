---
  root: true
  extends:
    airbnb
  parser: babel-eslint
  env:
    browser: true
    node: true
    protractor: true
    jasmine: true
    mocha: true
    es6: true
  rules:
    comma-dangle:
      - 2
      - "never"
    new-cap: 0
    no-underscore-dangle: 0
    import/extensions: 0
    import/prefer-default-export: 0
    import/no-unresolved: 0
    import/no-extraneous-dependencies: 0
